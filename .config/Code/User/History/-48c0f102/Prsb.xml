<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- pos.order.entry cron -->
        <record id="pos_order_entry_ir_cron" forcecreate="True" model="ir.cron">
            <field name="name">pos.order.entry.ir.cron</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="doall" eval="False"/>
            <field name="active" eval="True"/>
            <field name="interval_number">60</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_pos_order_entry"/>
            <field name="state">code</field>
            <field name="run_as_queue_job" eval="True"/>
            <field name="code">model.cron_convert_pos_order_entry()</field>
            <field name="priority" eval="5"/>
        </record>
    </data>
</odoo>


