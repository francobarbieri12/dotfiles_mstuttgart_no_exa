        <field name="name">invoice.electronic.event.view.tree</field>
        <field name="model">invoice.electronic.event</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add your fields here -->
                <field name="state" invisible="1"/>
                <field name="code"/>
                <field name="category"/>
                <field name="name"/>
                <field name="create_date"/>
                <field name="create_uid"/>
            </tree>
        </field>
    </record>
</odoo>