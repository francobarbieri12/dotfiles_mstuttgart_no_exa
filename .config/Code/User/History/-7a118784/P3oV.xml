<Rps>
    <InfDeclaracaoPrestacaoServico>
        <Rps>
            <IdentificacaoRps>
                <Numero>{{ rps.numero }}</Numero>
                <Serie>{{ rps.serie }}</Serie>
                <Tipo>{{ rps.tipo }}</Tipo>
            </IdentificacaoRps>
            <DataEmissao>{{ rps.data_emissao }}</DataEmissao>
            <Status>{{ rps.status }}</Status>
        </Rps>
        <Servico>
            <Valores>
                <ValorServicos>{{ rps.servico.valor_servicos }}</ValorServicos>
                <ValorPis>{{ rps.servico.valor_pis }}</ValorPis>
                <ValorCofins>{{ rps.servico.valor_confins }}</ValorCofins>
                <ValorInss>{{ rps.servico.valor_inss }}</ValorInss>
                <ValorCsll>{{ rps.servico.valor_csll }}</ValorCsll>
                <!-- <Aliquota>{{ rps.servico.aliquota }}</Aliquota> -->
            </Valores>
            <CodigoTributacaoMunicipio>{{ rps.servico.codigo_tributacao_municipio }}</CodigoTributacaoMunicipio>
            <Discriminacao>{{ rps.servico.discriminacao }}</Discriminacao>
            <CodigoMunicipio>{{ rps.servico.codigo_municipio }}</CodigoMunicipio>
        </Servico>
        <Prestador>
            <CpfCnpj>
                {% if rps.prestador.cnpj_cpf|length == 14  %}
                    <Cnpj>{{ rps.prestador.cnpj_cpf }}</Cnpj>
                {% endif %}
                {% if rps.prestador.cnpj_cpf|length == 11  %}
                    <Cpf>{{ rps.prestador.cnpj_cpf }}</Cpf>
                {% endif %}
            </CpfCnpj>
            <InscricaoMunicipal>{{ rps.prestador.inscricao_municipal }}</InscricaoMunicipal>
        </Prestador>
        <Tomador>
            <IdentificacaoTomador>
                <CpfCnpj>
                    {% if rps.tomador.cnpj_cpf|length == 14  %}
                        <Cnpj>{{ rps.tomador.cnpj_cpf }}</Cnpj>
                    {% endif %}
                    {% if rps.tomador.cnpj_cpf|length == 11  %}
                        <Cpf>{{ rps.tomador.cnpj_cpf }}</Cpf>
                    {% endif %}
                </CpfCnpj>
                {% if tomador.inscricao_municipal %}
                    <InscricaoMunicipal>{{ rps.tomador.inscricao_municipal }}</InscricaoMunicipal>
                {% endif %}
            </IdentificacaoTomador>
            <RazaoSocial>{{ rps.tomador.razao_social }}</RazaoSocial>
            <Endereco>
                <Endereco>{{ rps.tomador.endereco.rua }}</Endereco>
                <Numero>{{ rps.tomador.endereco.numero }}</Numero>
                <Complemento>{{ rps.tomador.endereco.complemento }}</Complemento>
                <Bairro>{{ rps.tomador.endereco.bairro }}</Bairro>
                <CodigoMunicipio>{{ rps.tomador.endereco.codigo_municipio }}</CodigoMunicipio>
                <Uf>{{ rps.tomador.endereco.uf }}</Uf>
            </Endereco>
        </Tomador>
    </InfDeclaracaoPrestacaoServico>
</Rps>
