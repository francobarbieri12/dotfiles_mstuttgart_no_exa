{
    "editor.renderWhitespace": "all",
    "breadcrumbs.enabled": true,
    "files.autoSave": "afterDelay",
    "files.exclude": {
        "**/__pycache__": true
    },
    "gitProjectManager.baseProjectsFolders": [
        "/home/michell/Workspace"
    ],
    "gitProjectManager.storeRepositoriesBetweenSessions": true,
    "gitProjectManager.ignoredFolders": [
        "others",
        "node_modules",
        "vendor"
    ],
    "gitProjectManager.checkRemoteOrigin": false,
    "window.titleBarStyle": "custom",
    "editor.selectionClipboard": false,
    "odooScaffold.odooBinPath": "/home/michell/Workspace/multidadosti-erp/odoo/multierp/src/core/odoo-bin",
    "workbench.tree.indent": 20,
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "terminal.integrated.detectLocale": "auto",
    "terminal.integrated.env.linux": {
        "LANG": "C.UTF-8"
    },
    "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "odooScaffold.pythonVirtualEnvFolder": "/home/michell/Workspace/multidadosti-erp/odoo/.venv/bin/python",
    "odooScaffold.pythonVirtualEnv": "/home/michell/dWorkspace/multidadosti-erp/odoo/.venv/bin/python",
    "editor.renderControlCharacters": true,
    "python.languageServer": "Pylance",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "yaml.customTags": [
        "!encrypted/pkcs1-oaep scalar",
        "!vault scalar"
    ],
    "redhat.telemetry.enabled": true,
    "ansible.ansibleLint.enabled": false,
    "workbench.preferredLightColorTheme": "Everforest Light",
    "workbench.preferredDarkColorTheme": "Everforest Dark",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "editor.codeLensFontFamily": "'JetBrainsMono Nerd Font Mono',",
    "editor.accessibilitySupport": "off",
    "terminal.integrated.useWslProfiles": false,
    "Notes.notesLocation": "/home/michell/Workspace/mstuttgart/notes/programming",
    "workbench.iconTheme": "ayu",
    "files.associations": {
        "*.py": "python"
    },
    "editor.formatOnType": true,
    "git.openRepositoryInParentFolders": "always",
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "editor.minimap.enabled": false,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.terminal.focusFind"
    ],
    "update.showReleaseNotes": false,
    "git.autofetch": true,
    "python.analysis.extraPaths": [
        "${workspaceFolder}/**/*"
    ],
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "python.analysis.autoFormatStrings": true,
    "workbench.colorTheme": "Everforest Light",
    "python.envFile": "${workspaceFolder}/.venv",
    "python.venvFolders": [
        ".venv"
    ],
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "vscode-neovim.neovimExecutablePaths.linux": "/snap/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.linux": "$HOME/.config/nvim",
    "python.analysis.completeFunctionParens": true,
    "vim.leader": ",",
    "editor.tokenColorCustomizations": {
        "[Everforest Light]": {
            "textMateRules": [
                {
                    "scope": [
                        "markup.admonition entity.name.function.asciidoc",
                        "variable.language.self.crystal",
                        "variable.language.special.self.python",
                        "variable.parameter.function.language.special.self.python",
                        "variable.language.this"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#df69ba"
                    }
                },
                {
                    "scope": "string, punctuation.definition.string.end, punctuation.definition.string.begin, punctuation.definition.string.template.begin, punctuation.definition.string.template.end",
                    "settings": {
                        "foreground": "#35a77c"
                    }
                },
                {
                    "scope": "support.module, support.node, support.other.module, support.type.object.module, entity.name.type.class.python,entity.name.type.module, entity.name.type.class.module, keyword.control.module",
                    "settings": {
                        "foreground": "#f57d26"
                    }
                }
            ]
        }
    }
}